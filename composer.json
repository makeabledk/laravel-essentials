{
    "name": "makeabledk/laravel-essentials",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelEssentials\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "illuminate/container": "6.*|7.*|8.*",
        "illuminate/http": "6.*|7.*|8.*",
        "illuminate/support": "6.*|7.*|8.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "laravel/laravel": "^8.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelEssentials\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makeable\\LaravelEssentials\\EssentialsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
