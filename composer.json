{
    "name": "makeabledk/laravel-essentials",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "Makeable\\LaravelEssentials\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "laravel/laravel": "5.5",
        "doctrine/dbal": "^2.5",
        "orchestra/testbench": "~3.5",
        "nunomaduro/collision": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Makeable\\LaravelEssentials\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "style": "./vendor/bin/php-cs-fixer fix ./src --rules=@PSR2,@Symfony,ordered_class_elements,ordered_imports --verbose"
    }
}
